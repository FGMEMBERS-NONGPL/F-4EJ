<?xml version="1.0"?>

<PropertyList>
	
	

<!--station left 1 -->



<model>
<name>leftDroptank</name>
<path>Aircraft/F-4EJ/Models/Stores/Tank_370/tank_370_sta1.xml</path>

<offsets>
<x-m> 0.3</x-m>
   <y-m>-3.65</y-m>
   <z-m>-0.85</z-m>
   <pitch-deg>-2</pitch-deg>
</offsets>
</model>	

<animation>
<type>select</type>
<object-name>leftDroptank</object-name>      
<condition>
<and>
<equals>
<property>sim/weight[0]/selected</property>
<value>370_Gal_tank</value>
</equals>
<equals>
<property>controls/armament/station[0]/release</property>
<value>false</value>
</equals>
</and>
</condition>
</animation>


	
